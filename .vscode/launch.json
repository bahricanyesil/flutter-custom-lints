{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Tests (FVM)",
      "type": "dart",
      "request": "launch",
      "program": "test/",
      "console": "terminal",
      "toolArgs": [
        "--enable-asserts"
      ],
      "vmAdditionalArgs": [
        "--enable-vm-service"
      ]
    },
    {
      "name": "Debug Single Test File (FVM)",
      "type": "dart",
      "request": "launch",
      "program": "${file}",
      "console": "terminal",
      "toolArgs": [
        "--enable-asserts"
      ],
      "vmAdditionalArgs": [
        "--enable-vm-service"
      ]
    }
  ]
}